<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/keyword_out_dialog"
    android:layout_width="216dp"
    android:layout_height="132dp"
    android:layout_centerHorizontal="true"
    android:layout_centerVertical="true"
    android:background="@drawable/keyword_audio_out_dialog_bg"

    >
    <TextView
        android:id="@+id/key_aud_out_question_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="오디오 저장에서 나가시겠어요?"
        android:textSize="14sp"
        android:textColor="#00051F"
        android:lineSpacingExtra="3sp"
        android:gravity="center_horizontal|center_vertical"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="32dp"/>
    <TextView
        android:id="@+id/key_aud_out_cancel_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/keyword_audio_out_btn_bg"
        android:backgroundTint="@color/main_color"
        android:text="취소"
        android:lineSpacingExtra="3sp"
        android:gravity="center_horizontal|center_vertical"
        android:textSize="14sp"
        android:textColor="@color/white"
        android:letterSpacing="0.01"
        app:layout_constraintStart_toStartOf="@id/key_aud_out_question_tv"
        app:layout_constraintTop_toBottomOf="@id/key_aud_out_question_tv"
        android:layout_marginTop="24dp"/>
    <TextView
        android:id="@+id/key_aud_out_confirm_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/keyword_audio_out_btn_bg"
        android:backgroundTint="#ECEFF1"
        android:text="나가기"
        android:lineSpacingExtra="3sp"
        android:gravity="center_horizontal|center_vertical"
        android:textSize="14sp"
        android:textColor="#00051F"
        android:letterSpacing="0.01"
        app:layout_constraintEnd_toEndOf="@id/key_aud_out_question_tv"
        app:layout_constraintTop_toBottomOf="@id/key_aud_out_question_tv"
        android:layout_marginTop="24dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>